@page
@model HighSchoolApplication.Pages.Teacher.DeleteModel
@{
}
@if (Model.IsSuccess)
{
    <div class="alert alert-success">
        You succesfully deleted id @Model.Id
    </div>
    <div>
        <a class="btn btn-primary " asp-page="./Edit">Add a New Teacherudent</a>
        <a class="btn btn-primary " asp-page="/Teacher/Index">Return to Teacher Page</a>
    </div>
}
else
{
    <div class="alert alert-success">
        There was an issue deleting id @Model.Id
    </div>
    <div>
        <a class="btn btn-primary " asp-page="./Edit">Add a New Teacher</a>
        <a class="btn btn-primary " asp-page="/Teacher/Index">Return to Teacher Page</a>
    </div>
}